<h1>新規コスメ投稿</h1>
<%= form_with model: @post, local:true do |f| %>
    <h4>商品名</h4>
    <%= f.text_field :location %>
    <h4>カテゴリ</h4>
     <%= f.text_field :location %>
    <h4>レビュー</h4>
    <%= f.text_area :text %>
    <h4>画像</h4>
    <%= f.file_field :image %>
    <p></p>
    <%= f.submit '投稿する' %>
<% end %>